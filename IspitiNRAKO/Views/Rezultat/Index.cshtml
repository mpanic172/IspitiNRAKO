@model IEnumerable<IspitiNRAKO.Rezultat>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Moji rezultati</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Vrijeme)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prolaz)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BrojBodova)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Postotak)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ispit.Naziv)
        </th>
        <th>
            Korisnik
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Vrijeme)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Prolaz)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BrojBodova)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Postotak)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ispit.Naziv)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Korisnik.Username)
        </td>
        <td>
            
        </td>
    </tr>
}

</table>
